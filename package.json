{
  "name": "js-source-extractor",
  "version": "1.0.0",
  "description": "Extracts source from javascript frameworks with sourcemap information (containing the source)",
  "main": "js-source-extractor.js",
  "scripts": {
    "clean": "rimraf dist",
    "test": "mocha -r ts-node/register src/**/*.spec.ts",
    "build": "tsc -p tsconfig-dist.json"
  },
  "keywords": [
    "javascript",
    "source-map",
    "source map",
    "extractor",
    "extract",
    "export",
    "source code",
    "code"
  ],
  "author": "Kay Huber",
  "license": "ISC",
  "repository": "https://github.com/unblu/js-source-extractor.git",
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/chai-as-promised": "^7.1.0",
    "@types/mime": "^2.0.0",
    "@types/mocha": "^5.2.2",
    "@types/node": "^10.3.4",
    "@types/rimraf": "^2.0.2",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "mocha": "^5.2.0",
    "rimraf": "^2.6.2",
    "ts-node": "^6.1.2",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "mime": "^2.3.1",
    "source-map": "^0.7.3"
  }
}
